  "devDependencies": {
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "jsdom": "^25.0.1",
    "prettier": "^3.6.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "turbo": "^2.5.5",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.6.1",
  "engines": {
    "node": ">=20"
  }
}
