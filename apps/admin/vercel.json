{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://api.redot.me/api/:path*"
    },
    {
      "source": "/api/:path*/",
      "destination": "https://api.redot.me/api/:path*/"
    },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
