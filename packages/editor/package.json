{
  "name": "@repo/editor",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsup --dts --minify",
    "lint": "eslint ."
  },
  "files": [
    "dist",
    "src/**/*.scss"
  ],
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./components": {
      "require": {
        "types": "./dist/components/index.d.cts",
        "default": "./dist/components/index.cjs"
      },
      "import": {
        "types": "./dist/components/index.d.ts",
        "default": "./dist/components/index.js"
      }
    },
    "./components/ui": {
      "require": {
        "types": "./dist/components/ui/index.d.cts",
        "default": "./dist/components/ui/index.cjs"
      },
      "import": {
        "types": "./dist/components/ui/index.d.ts",
        "default": "./dist/components/ui/index.js"
      }
    },
    "./components/ui-primitive": {
      "require": {
        "types": "./dist/components/ui-primitive/index.d.cts",
        "default": "./dist/components/ui-primitive/index.cjs"
      },
      "import": {
        "types": "./dist/components/ui-primitive/index.d.ts",
        "default": "./dist/components/ui-primitive/index.js"
      }
    },
    "./components/node": {
      "require": {
        "types": "./dist/components/node/index.d.cts",
        "default": "./dist/components/node/index.cjs"
      },
      "import": {
        "types": "./dist/components/node/index.d.ts",
        "default": "./dist/components/node/index.js"
      }
    },
    "./components/icons": {
      "require": {
        "types": "./dist/components/icons/index.d.cts",
        "default": "./dist/components/icons/index.cjs"
      },
      "import": {
        "types": "./dist/components/icons/index.d.ts",
        "default": "./dist/components/icons/index.js"
      }
    },
    "./lib": {
      "require": {
        "types": "./dist/lib/index.d.cts",
        "default": "./dist/lib/index.cjs"
      },
      "import": {
        "types": "./dist/lib/index.d.ts",
        "default": "./dist/lib/index.js"
      }
    },
    "./hooks": {
      "require": {
        "types": "./dist/hooks/index.d.cts",
        "default": "./dist/hooks/index.cjs"
      },
      "import": {
        "types": "./dist/hooks/index.d.ts",
        "default": "./dist/hooks/index.js"
      }
    },
    "./styles": {
      "require": {
        "types": "./dist/styles.d.cts",
        "default": "./dist/styles.cjs"
      },
      "import": {
        "types": "./dist/styles.d.ts",
        "default": "./dist/styles.js"
      }
    }
  },
  "sideEffects": [
    "**/*.scss",
    "**/*.css",
    "./dist/styles.js",
    "./dist/styles.cjs"
  ],
  "dependencies": {
    "@floating-ui/react": "^0.27.16",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@tiptap/extension-bullet-list": "^3.9.0",
    "@tiptap/extension-highlight": "^3.8.0",
    "@tiptap/extension-horizontal-rule": "^3.8.0",
    "@tiptap/extensions": "^3.8.0",
    "@tiptap/pm": "^3.8.0",
    "@tiptap/react": "^3.8.0",
    "@tiptap/starter-kit": "^3.8.0",
    "lodash.throttle": "^4.1.1",
    "react-hotkeys-hook": "^5.2.1"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/lodash.throttle": "^4.1.9",
    "@types/react": "^19.0.0",
    "sass": "^1.93.2",
    "tsup": "^8.5.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "react": "^19.0.0"
  }
}
